{
  "version": 2,
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/cron/keep-alive",
      "schedule": "*/5 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/stores/:slug",
      "destination": "/stores/[slug]"
    },
    {
      "source": "/categories/:slug",
      "destination": "/categories/[slug]"
    },
    {
      "source": "/promocodes/:id",
      "destination": "/promocodes/[id]"
    }
  ],
  "redirects": [
    {
      "source": "gopromocodes.com",
      "destination": "https://www.gopromocodes.com",
      "permanent": true
    }
  ]
}
